<!DOCTYPE html>
<html>
<head>
  <title>UrlChomp</title>
  <%= stylesheet_link_tag   'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css',
                            'https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css',
                            'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js',
                               'http://code.highcharts.com/highcharts.js',
                              'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render partial: 'layouts/nav' %>
  <div class="container">
    <%= flash_messages %>
    <%= yield %>
  </div>
</body>
</html>
